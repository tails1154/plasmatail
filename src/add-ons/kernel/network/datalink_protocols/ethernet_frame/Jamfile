SubDir HAIKU_TOP src add-ons kernel network datalink_protocols ethernet_frame ;

UsePrivateKernelHeaders ;
UsePrivateHeaders net ;

KernelAddon ethernet_frame :
	ethernet_frame.cpp
;

# Installation
PlasmatailInstall install-networking : /boot/home/config/add-ons/kernel/haiku_network/datalink_protocols
	: ethernet_frame ;
